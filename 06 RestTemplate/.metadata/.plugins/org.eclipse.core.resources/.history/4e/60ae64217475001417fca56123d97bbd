package com.cursoandroid.resttemplate;

public class User {
	private String name;
	private String email;
	private String id;

	public User(String id, String nombre, String email) {
		super();
		this.name = nombre;
		this.email = email;
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getId() {
		return id;
	}

	public void setId(String) {
		this.id = id;
	}

}
